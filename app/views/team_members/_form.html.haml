= form_for(team_team_members_path) do |f|
  - if team_member.errors.any?
    #error_explanation
      %h2
        = pluralize(team_member.errors.count, "error")
        prohibited this team_member from being saved:
      %ul
        - team_member.errors.full_messages.each do |message|
          %li= message
  .field
    = f.label :role
    = f.number_field :role
  .field
    = f.label :team_id
    = f.number_field :team_id
  .field
    = f.label :user_id
    = f.number_field :user_id
  .actions
    = f.submit
